{
  "extends": "./.quasar/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "module": "ESNext",
    "target": "ESNext",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "assets/*": ["src/assets/*"],
      "boot": ["src/boot"],
      "boot/*": ["src/boot/*"],
      "stores": ["src/stores"],
      "stores/*": ["src/stores/*"],
      "i18n": ["src/i18n"],
      "i18n/*": ["src/i18n/*"],
      "ui": ["src/ui"],
      "ui/*": ["src/ui/*"],
      "#q-app": ["./node_modules/@quasar/app-vite/types/index.d.ts"],
      "#q-app/wrappers": ["./node_modules/@quasar/app-vite/types/app-wrappers.d.ts"],
      "#q-app/bex/background": ["./node_modules/@quasar/app-vite/types/bex/entrypoints/background.d.ts"],
      "#q-app/bex/content": ["./node_modules/@quasar/app-vite/types/bex/entrypoints/content.d.ts"],
      "#q-app/bex/private/bex-bridge": ["./node_modules/@quasar/app-vite/types/bex/bex-bridge.d.ts"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "eslint.config.js", "quasar.config.ts"],
  "exclude": ["node_modules"]
}
